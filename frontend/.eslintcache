ctReducers.js",[],"/Users/sontran/Desktop/reactjs/ecommerce/frontend/src/reducers/userReducers.js",[],["116","117"],"/Users/sontran/Desktop/reactjs/ecommerce/frontend/src/screens/SigninScreen.js",["118"],"import React, { useState, useEffect } from \"react\";\nimport { Link, useHistory } from \"react-router-dom\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { signin } from \"../actions/userAction\";\n\nexport const SigninScreen = (props) => {\n  const [email, setEmail] = useState(\"\");\n  const [password, setPassword] = useState(\"\");\n  const userSignIn = useSelector((state) => state.userSignin);\n  const { loading, userInfo, error } = userSignIn;\n  const dispatch = useDispatch();\n  const history = useHistory();\n  const redirect = props.location.search;\n  useEffect(() => {\n    if (userInfo) {\n      history.push(redirect);\n    }\n  }, [userInfo]);\n\n  const submitHandler = (e) => {\n    e.preventDefault();\n    dispatch(signin(email, password));\n  };\n  return (\n    <div className=\"form-signin\">\n      <form onSubmit={submitHandler}>\n        <ul className=\"form-container-signin\">\n          <li>\n            <h2>Sign-In</h2>\n          </li>\n          <li>\n            {loading && <div>Loading...</div>}\n            {error && <div>{\"Invalid Email or Password\"}</div>}\n          </li>\n          <li>\n            <label htmlFor=\"email\">Email</label>\n            <input\n              type=\"email\"\n              name=\"email\"\n              id=\"email\"\n              onChange={(e) => setEmail(e.target.value)}\n            />\n          </li>\n          <li>\n            <label htmlFor=\"password\">Password</label>\n            <input\n              type=\"password\"\n              name=\"password\"\n              id=\"password\"\n              onChange={(e) => setPassword(e.target.value)}\n            />\n          </li>\n          <li>\n            <button type=\"submit\" className=\"button primary\">\n              Signin\n            </button>\n          </li>\n          <li>New to Rommache?</li>\n          <li>\n            <Link\n              to={\n                redirect === \"/\" ? \"register\" : \"register?redirect=\" + redirect\n              }\n              className=\"button secondary text-center\"\n            >\n              Create An Account\n            </Link>\n          </li>\n        </ul>\n      </form>\n    </div>\n  );\n};\n","/Users/sontran/Desktop/reactjs/ecommerce/frontend/src/screens/RegisterScreen.js",["119"],"import React, { useState, useEffect } from \"react\";\nimport { Link } from \"react-router-dom\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { register } from \"../actions/userAction\";\n\nexport const RegisterScreen = (props) => {\n  const [name, setName] = useState(\"\");\n  const [email, setEmail] = useState(\"\");\n  const [password, setPassword] = useState(\"\");\n  const [rePassword, setRePassword] = useState(\"\");\n  const userRegister = useSelector((state) => state.userRegister);\n  const { loading, userInfo, error } = userRegister;\n  const dispatch = useDispatch();\n\n  useEffect(() => {\n    if (userInfo) {\n      props.history.push(\"/\");\n    }\n  }, [userInfo]);\n\n  const submitHandler = (e) => {\n    e.preventDefault();\n    if (password !== rePassword) {\n      alert(\"Password and confirm password are not match\");\n    } else {\n      dispatch(register(name, email, password));\n    }\n  };\n  return (\n    <div className=\"form-signin\">\n      <form onSubmit={submitHandler}>\n        <ul className=\"form-container-signin\">\n          <li>\n            <h2>Register</h2>\n          </li>\n          <li>\n            {loading && <div>Loading...</div>}\n            {error && <div>{error}</div>}\n          </li>\n          <li>\n            <label htmlFor=\"name\">Name</label>\n            <input\n              type=\"name\"\n              name=\"name\"\n              id=\"name\"\n              onChange={(e) => setName(e.target.value)}\n            />\n          </li>\n          <li>\n            <label htmlFor=\"email\">Email</label>\n            <input\n              type=\"email\"\n              name=\"email\"\n              id=\"email\"\n              onChange={(e) => setEmail(e.target.value)}\n            />\n          </li>\n          <li>\n            <label htmlFor=\"password\">Password</label>\n            <input\n              type=\"password\"\n              name=\"password\"\n              id=\"password\"\n              onChange={(e) => setPassword(e.target.value)}\n            />\n          </li>\n          <li>\n            <label htmlFor=\"rePassword\">Re-Enter Password</label>\n            <input\n              type=\"password\"\n              name=\"rePassword\"\n              id=\"rePassword\"\n              onChange={(e) => setRePassword(e.target.value)}\n            />\n          </li>\n          <li>\n            <button type=\"submit\" className=\"button primary\">\n              Register\n            </button>\n          </li>\n          <li>\n            Already Have An Account? <Link to=\"/signin\">Sign-In</Link>\n          </li>\n        </ul>\n      </form>\n    </div>\n  );\n};\n","/Users/sontran/Desktop/reactjs/ecommerce/frontend/src/components/Cart.js",["120"],"import React, { useEffect, useState } from \"react\";\nimport formatCurrency from \"../util\";\nimport Fade from \"react-reveal/Fade\";\nimport { useSelector, useDispatch } from \"react-redux\";\nimport Modal from \"react-modal\";\nimport Zoom from \"react-reveal/Zoom\";\nimport { removeFromCart } from \"../actions/cartActions\";\nimport { createOrder, clearOrder } from \"../actions/orderActions\";\nimport PaypalButtons from \"./PaypalButtons\";\n\nexport const Cart = (props) => {\n  const [info, setInfo] = useState({\n    name: \"\",\n    email: \"\",\n    address: \"\",\n  });\n  const [showCheckout, setShowCheckout] = useState(false);\n  const [showPaypal, setShowPaypal] = useState(false);\n\n  const orderSel = useSelector((state) => state.order);\n  const cartSel = useSelector((state) => state.cart);\n\n  const { order } = orderSel;\n  const { cartItems } = cartSel;\n\n  const dispatch = useDispatch();\n\n  const handleInput = (e) => {\n    const { value, name } = e.target;\n    setInfo({\n      ...info,\n      [name]: value,\n    });\n  };\n  const makeOrder = (e) => {\n    e.preventDefault();\n    const order = {\n      name: info.name,\n      email: info.email,\n      address: info.address,\n      cartItems: cartItems,\n      total: cartItems.reduce((a, c) => a + c.price * c.count, 0),\n    };\n    dispatch(createOrder(order));\n  };\n  const closeModal = () => {\n    dispatch(clearOrder());\n  };\n  const showPaypalButtons = () => {\n    setShowPaypal(true);\n  };\n  return (\n    <div>\n      {cartItems.length === 0 ? (\n        <div className=\"cart cart-header\">Cart is empty</div>\n      ) : (\n        <div className=\"cart cart-header\">\n          You have {cartItems.length} in the cart{\" \"}\n        </div>\n      )}\n\n      {order && (\n        <Modal isOpen={true} onRequestClose={closeModal}>\n          <Zoom>\n            <button className=\"close-modal\" onClick={closeModal}>\n              x\n            </button>\n            {showPaypal ? (\n              <div className=\"paypal-btn\">\n                <PaypalButtons amount={formatCurrency(order.total)} />\n              </div>\n            ) : (\n              <div className=\"order-details\">\n                <h3 className=\"success-message\">Your order has been placed.</h3>\n                <h2>Order {order._id}</h2>\n                <ul>\n                  <li>\n                    <div>Name:</div>\n                    <div>{order.name}</div>\n                  </li>\n                  <li>\n                    <div>Email:</div>\n                    <div>{order.email}</div>\n                  </li>\n                  <li>\n                    <div>Address:</div>\n                    <div>{order.address}</div>\n                  </li>\n                  <li>\n                    <div>Date:</div>\n                    <div>{order.createdAt}</div>\n                  </li>\n                  <li>\n                    <div>Total:</div>\n                    <div>{\"$\" + formatCurrency(order.total)}</div>\n                  </li>\n                  <li>\n                    <div>Cart Items:</div>\n                    <div>\n                      {order.cartItems.map((x) => (\n                        <div>\n                          {x.count} {\" x \"} {x.title}\n                        </div>\n                      ))}\n                    </div>\n                  </li>\n                </ul>\n                <div>\n                  <button\n                    className=\"button primary\"\n                    onClick={showPaypalButtons}\n                  >\n                    Pay {\"$\" + formatCurrency(order.total)}\n                  </button>\n                </div>\n              </div>\n            )}\n          </Zoom>\n        </Modal>\n      )}\n      <div>\n        <div className=\"cart\">\n          <Fade left cascade>\n            <ul className=\"cart-items\">\n              {cartItems.map((item) => (\n                <li key={item._id}>\n                  <div>\n                    <img src={item.image} alt={item.title}></img>\n                  </div>\n                  <div>\n                    <div>{item.title}</div>\n                    <div className=\"right\">\n                      {\"$\" + formatCurrency(item.price)} x {item.count}{\" \"}\n                      <button\n                        className=\"button\"\n                        onClick={() => dispatch(removeFromCart(item))}\n                      >\n                        Remove\n                      </button>\n                    </div>\n                  </div>\n                </li>\n              ))}\n            </ul>\n          </Fade>\n        </div>\n        {cartItems.length !== 0 && (\n          <div>\n            <div className=\"cart\">\n              <div className=\"total\">\n                <div>\n                  Total:{\" \"}\n                  {\"$\" +\n                    formatCurrency(\n                      cartItems.reduce((a, c) => a + c.price * c.count, 0)\n                    )}\n                </div>\n                <button\n                  onClick={() => {\n                    setShowCheckout(true);\n                  }}\n                  className=\"button primary\"\n                >\n                  Proceed\n                </button>\n              </div>\n            </div>\n            {showCheckout && (\n              <Fade right cascade>\n                <div className=\"cart\">\n                  <form onSubmit={makeOrder}>\n                    <ul className=\"form-container\">\n                      <li>\n                        <label>Email</label>\n                        <input\n                          name=\"email\"\n                          type=\"email\"\n                          required\n                          onChange={handleInput}\n                        ></input>\n                      </li>\n                      <li>\n                        <label>Name</label>\n                        <input\n                          name=\"name\"\n                          type=\"text\"\n                          required\n                          onChange={handleInput}\n                        ></input>\n                      </li>\n                      <li>\n                        <label>Address</label>\n                        <input\n                          name=\"address\"\n                          type=\"text\"\n                          required\n                          onChange={handleInput}\n                        ></input>\n                      </li>\n                      <li>\n                        <button className=\"button primary\" type=\"submit\">\n                          Checkout\n                        </button>\n                      </li>\n                    </ul>\n                  </form>\n                </div>\n              </Fade>\n            )}\n          </div>\n        )}\n      </div>\n    </div>\n  );\n};\n","/Users/sontran/Desktop/reactjs/ecommerce/frontend/src/reducers/orderReducers.js",[],"/Users/sontran/Desktop/reactjs/ecommerce/frontend/src/screens/AdminScreen.js",["121"],"import React, { Component } from \"react\";\nimport { Orders } from \"../components/Orders\";\n\nexport const AdminScreen = () => {\n  return (\n    <div>\n      <Orders />\n    </div>\n  );\n};\n","/Users/sontran/Desktop/reactjs/ecommerce/frontend/src/components/Orders.js",["122","123"],"/Users/sontran/Desktop/reactjs/ecommerce/frontend/src/screens/HomeScreen.js",["124"],"import React, { Component } from \"react\";\nimport { Filter } from \"../components/Filter\";\nimport {Products} from \"../components/Products\";\nimport { Cart } from \"../components/Cart\";\n\nexport const HomeScreen = () => {\n  return (\n    <div>\n      <div className=\"content\">\n        <div className=\"main\">\n          <Filter></Filter>\n          <Products></Products>\n        </div>\n        <div className=\"sidebar\">\n          <Cart />\n        </div>\n      </div>\n    </div>\n  );\n};\n","/Users/sontran/Desktop/reactjs/ecommerce/frontend/src/store.js",[],"/Users/sontran/Desktop/reactjs/ecommerce/frontend/src/components/Products.js",["125"],"import React, { useEffect, useState } from \"react\";\nimport formatCurrency from \"../util\";\nimport Fade from \"react-reveal/Fade\";\nimport Modal from \"react-modal\";\nimport Zoom from \"react-reveal/Zoom\";\nimport { useSelector, useDispatch } from \"react-redux\";\nimport { fetchProducts } from \"../actions/productActions\";\nimport { addToCart } from \"../actions/cartActions\";\n\nexport const Products = (props) => {\n  const [product, setProduct] = useState(nul